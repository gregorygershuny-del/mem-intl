[32m+[m[32mfunction Stat({ title, value, note }: { title: string; value: string; note: string }) {[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="rounded-2xl border border-neutral-200 p-4">[m
[32m+[m[32m      <p className="text-xs text-neutral-500">{title}</p>[m
[32m+[m[32m      <p className="text-xl font-semibold mt-1">{value}</p>[m
[32m+[m[32m      <p className="text-xs text-neutral-500 mt-0.5">{note}</p>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction ServiceCard({ title, bullets }: { title: string; bullets: string[] }) {[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="rounded-3xl shadow-sm hover:shadow-md transition-shadow border border-neutral-200 bg-white p-6">[m
[32m+[m[32m      <h4 className="text-lg font-semibold">{title}</h4>[m
[32m+[m[32m      <ul className="mt-3 grid gap-2 text-sm text-neutral-700">[m
[32m+[m[32m        {bullets.map((b, i) => ([m
[32m+[m[32m          <li key={i} className="flex items-start gap-2">‚úî {b}</li>[m
[32m+[m[32m        ))}[m
[32m+[m[32m      </ul>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction Industry({ title, desc }: { title: string; desc: string }) {[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="rounded-3xl shadow-sm border border-neutral-200 bg-white p-6">[m
[32m+[m[32m      <h5 className="font-semibold">{title}</h5>[m
[32m+[m[32m      <p className="text-sm text-neutral-600 mt-1">{desc}</p>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction MiniStat({ label, value }: { label: string; value: string }) {[m
[32m+[m[32m  return ([m
[32m+[m[32m    <div className="rounded-2xl border border-neutral-200 p-4 bg-white">[m
[32m+[m[32m      <p className="text-xs text-neutral-500">{label}</p>[m
[32m+[m[32m      <p className="text-base font-semibold">{value}</p>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
